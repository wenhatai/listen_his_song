	color: #CFCFCF;
	overflow: auto;
}
#title{
	font-size: 18px;
	font-weight: bold;
	line-height: 26px;
	padding-left: 10px;
	border-bottom: 1px #000 solid;
}
.grouping{
	font-size: 14px;
	font-weight: bold;
	line-height: 24px;
	border-bottom: 1px #000 solid;	
	padding-left: 32px;
	background: url(../image/unfold.png) 6px 4px no-repeat;
}
.list{
	display: none;
}
.choose{
	background: url(../image/fold.png) 6px 4px no-repeat;
}
.panel{
	font-size: 14px;
	font-weight: bold;
	line-height: 24px;
	display: none;
}
.list li{
	font-size: 12px;
	font-weight: normal;
	line-height: 24px;
	border-top: 1px #111 solid;
	padding: 5px 0;
	margin-left: -27px;
}
.list li img{
	height: 24px;
	margin-right: 10px;
	float: left;	
}
