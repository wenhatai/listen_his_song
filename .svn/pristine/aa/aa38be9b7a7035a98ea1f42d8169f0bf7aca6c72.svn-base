/*css reset*/
body, div, dl, dt, dd, ul, ol, li,
h1, h2, h3, h4, h5, h6, pre, code,
form, fieldset, legend, input, button,
textarea, p, blockquote, th, td {
    margin: 0;
    padding: 0;
}
img {
    border: 0;
}
:focus {
    outline: 0;
}
h1, h3, h4, h5, h6 {
    font-size: 100%;
    font-weight: normal;
}
input, button, textarea,
select, optgroup, option {
    font-family: inherit;
    font-size: inherit;
    font-style: inherit;
    font-weight: inherit;
}
ol, ul {
    list-style: none;
}
:link, :visited , ins {
    text-decoration: none;
}
/*main css*/
body{
	font-family: "宋体", Arial, Helvetica, sans-serif ; 
	font-size:  12px;
	line-height: 18px;
	background-color: #ECEEEA;
}
#main{
	padding: 20px 32px;
	width: 960px;
	margin: 0 auto;
}

/*left part*/
#left{
	width: 174px;
	float: left;
}
#uers{
	width: 174px;
	height: 88px;
}
#userhead{
	float: left;
	width: 69px;
	height: 69px;
	margin-right: 10px;
}
#friendlist{
	background-color: #3A3A3A;
